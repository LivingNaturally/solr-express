{
  "buildOptions": {
    "allowUnsafe": true,
    "embed": [ "Query/Result/*.json" ],
    "keyFile": "../../Key.snk",
    "strongName": true,
    "warningsAsErrors": true
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true
      }
    }
  },
  "dependencies": {
    "BenchmarkDotNet": "0.9.8",
    "SolrExpress.Benchmarks.Helper": {
      "target": "project"
    },
    "SolrExpress.Solr4": {
      "target": "project"
    }
  },
  "frameworks": {
    "net45": {
      "buildOptions": {
        "define": [ "CLASSIC" ]
      },
      "dependencies": {
        "Moq": "4.5.19",
        "System.Runtime": "4.0.0",
        "System.Threading.Tasks": "4.0.0"
      }
    },
    "netcoreapp1.0": {
      "buildOptions": {
        "define": [ "CORE" ]
      },
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.1",
          "type": "platform"
        },
        "Moq": "4.6.36-alpha"
      }
    }
  }
}