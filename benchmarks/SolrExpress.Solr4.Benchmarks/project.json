{
  "buildOptions": {
    "allowUnsafe": true,
    "keyFile": "../../Key.snk",
    "strongName": true,
    "warningsAsErrors": true
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true
      }
    }
  },
  "dependencies": {
    "BenchmarkDotNet": "0.9.8",
    "Moq": "4.6.36-alpha",
    "SolrExpress.Benchmarks.Helper": {
      "target": "project"
    },
    "SolrExpress.Solr4": {
      "target": "project"
    }
  },
  "frameworks": {
    "net45": {
      "buildOptions": {
        "define": [ "CLASSIC" ]
      },
      "frameworkAssemblies": {
        "System.Reflection": "4.0.0.0"
      }
    },
    "netcoreapp1.0": {
      "buildOptions": {
        "define": [ "CORE" ]
      },
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.0",
          "type": "platform"
        }
      }
    }
  }
}