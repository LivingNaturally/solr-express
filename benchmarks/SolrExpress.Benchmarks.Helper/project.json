{
  "buildOptions": {
    "allowUnsafe": true,
    "keyFile": "../../Key.snk",
    "strongName": true,
    "warningsAsErrors": true
  },
  "dependencies": {
    "SolrExpress.Core": {
      "target": "project"
    }
  },
  "frameworks": {
    "net45": {
      "buildOptions": {
        "define": [ "CLASSIC" ]
      },
      "frameworkAssemblies": {
        "System.Reflection": "4.0.0.0"
      }
    },
    "netcoreapp1.0": {
      "buildOptions": {
        "define": [ "CORE" ]
      },
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.1",
          "type": "platform"
        }
      }
    }
  }
}
