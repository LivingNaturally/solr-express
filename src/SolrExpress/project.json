{
  "authors": [ "diego.lb" ],
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../Key.snk"
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true
      }
    }
  },
  "description": "A simple and lightweight query .NET library for Solr, in a controlled, buildable and fail fast way",
  "frameworks": {
    "net45": {
      "dependencies": {
        "Flurl.Http.Signed": "1.1.1",
        "Newtonsoft.Json": "9.0.1"
      }
    },
    "net46": {
      "dependencies": {
        "Flurl.Http.Signed": "1.1.1",
        "Newtonsoft.Json": "9.0.1"
      }
    },
    "netstandard1.6": {
      "dependencies": {
        "Flurl.Http.Signed": "1.1.1",
        "NETStandard.Library": "1.6.0",
        "Newtonsoft.Json": "9.0.1"
      },
      "buildOptions": {
        "define": [ "NETCORE" ]
      }
    }
  },
  "packOptions": {
    "licenseUrl": "https://github.com/solr-express/solr-express/blob/master/LICENSE",
    "repository": {
      "url": "https://github.com/solr-express/solr-express",
      "type": "git"
    },
    "tags": [ "SOLR", "C#", ".Net Framework", ".Net Core" ],
    "requireLicenseAcceptance": false
  },
  "version": "5.0.0"
}