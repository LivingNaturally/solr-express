{
  "authors": [ "diego.lb" ],
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../Key.snk"
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true
      }
    }
  },
  "description": "A simple and lightweight query .NET library for Solr, in a controlled, buildable and fail fast way - Core library",
  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.Extensions.DependencyInjection": "1.0.0",
        "Microsoft.Extensions.Options": "1.0.0",
        "Newtonsoft.Json": "9.0.1",
        "System.Console": "4.0.0",
        "System.Diagnostics.Tools": "4.0.1",
        "System.IO": "4.1.0",
        "System.IO.FileSystem": "4.0.1"
      }
    },
    "net4": {
      "dependencies": {
        "Newtonsoft.Json": "9.0.1",
        "SimpleInjector": "3.2.0"
      }
    },
    "net45": {
      "dependencies": {
        "Newtonsoft.Json": "9.0.1",
        "SimpleInjector": "3.2.0"
      }
    }
  },
  "packOptions": {
    "licenseUrl": "https://github.com/solr-express/solr-express/blob/master/LICENSE",
    "repository": {
      "url": "https://github.com/solr-express/solr-express",
      "type": "git"
    },
    "tags": [ "SOLR", "C#", ".Net Framework", ".Net Core" ],
    "requireLicenseAcceptance": false
  },
  "version": "4.0.3"
}