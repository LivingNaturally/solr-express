{
  "authors": [ "diego.lb" ],
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../Key.snk"
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true
      }
    }
  },
  "dependencies": {
    "SolrExpress.Core": {
      "target": "project"
    }
  },
  "description": "A simple and lightweight query .NET library for Solr, in a controlled, buildable and fail fast way for Sor 4.x",
  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.Extensions.DependencyInjection.Abstractions": "1.0.0",
        "Microsoft.Extensions.Options": "1.0.0",
        "System.Net.Requests": "4.0.11",
        "System.Threading.Tasks.Parallel": "4.3.0"
      }
    },
    "net4": {
      "dependencies": {
        "Newtonsoft.Json": "9.0.1"
      }
    },
    "net45": {
      "dependencies": {
        "System.Threading.Tasks.Parallel": "4.3.0"
      }
    }
  },
  "packOptions": {
    "licenseUrl": "https://github.com/solr-express/solr-express/blob/master/LICENSE",
    "repository": {
      "url": "https://github.com/solr-express/solr-express",
      "type": "git"
    },
    "tags": [ "SOLR", "C#", ".Net Framework", ".Net Core" ],
    "requireLicenseAcceptance": false
  },
  "version": "4.0.7"
}