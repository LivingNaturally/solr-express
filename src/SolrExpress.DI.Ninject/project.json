{
  "authors": [ "diego.lb" ],
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../Key.snk"
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true
      }
    }
  },
  "dependencies": {
    "SolrExpress": {
      "target": "project"
    }
  },
  "description": "A simple and lightweight query .NET library for Solr, in a controlled, buildable and fail fast way. DI using Ninject mechanism",
  "frameworks": {
    "net45": {
      "dependencies": {
        "Ninject": "3.2.2"
      }
    },
    "net46": {
      "dependencies": {
        "Ninject": "3.2.2"
      }
    },
    //"netstandard1.6": {
    //  "dependencies": {
    //    "Ninject": "4.0.0-beta-0130"
    //  }
    //}
  },
  "packOptions": {
    "licenseUrl": "https://github.com/solr-express/solr-express/blob/master/LICENSE",
    "repository": {
      "url": "https://github.com/solr-express/solr-express",
      "type": "git"
    },
    "tags": [ "SOLR", "C#", ".Net Framework", ".Net Core" ],
    "requireLicenseAcceptance": false
  },
  "version": "5.0.0"
}